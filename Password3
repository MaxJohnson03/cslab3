password = input("Enter a password between 8 and 12 characters long. ")
password2 = input("Confirm password. ")
bad_passwords =  ['password', 'letmein', 'sesame', 'hello', 'justinbieber']
if 8 > len(password):
    print("Error, password too short. ")
elif 12 < len(password):
    print("Error, password too long. ")
elif password != password2:
    print("Error, passwords are not the same")
elif password in bad_passwords:
    print("Error, password is too predictable. ")
else:
    print("Password set")
